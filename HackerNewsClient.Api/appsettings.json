{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "ConnectionString": "Data Source=./hackernews.db"
  },
  "HackerNewsAPI": {
    "BestStoriesUrl": "https://hacker-news.firebaseio.com/v0/beststories.json",
    "StoryDetailsUrlFormat": "https://hacker-news.firebaseio.com/v0/item/{0}.json"
  },
  "Cache": {
    "Enabled": true,
    "AbsoluteExpirationRelativeToNow": "00:05:00"
  }
}
